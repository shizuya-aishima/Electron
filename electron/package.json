{
	"name": "electron",
	"description": "electron",
	"version": "0.1.0",
	"license": "MIT",
	"author": {
		"name": "node"
	},
	"repository": {
		"url": "https://github.com/node/electron.git"
	},
	"main": "dist/main.js",
	"scripts": {
		"dev": "rimraf dist && run-p dev:webpack dev:electron",
		"build": "rimraf dist && cross-env NODE_ENV=\"production\" webpack --progress",
		"dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
		"dev:electron": "wait-on dist/main.js dist/index.html && electronmon .",
		"build:win": "electron-packager . --platform=win32 --arch=x64 --overwrite"
	},
	"electronmon": {
		"patterns": [
			"dist/**/*"
		]
	},
	"dependencies": {
		"@headlessui/react": "^1.7.17",
		"@heroicons/react": "^2.0.18",
		"node-sass": "^9.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-router-dom": "6"
	},
	"devDependencies": {
		"@types/node": "^20.5.9",
		"@types/react": "^18.2.21",
		"@types/react-dom": "^18.2.7",
		"autoprefixer": "^10.4.15",
		"cross-env": "^7.0.3",
		"css-loader": "^6.8.1",
		"electron": "^26.1.0",
		"electron-packager": "^17.1.2",
		"electronmon": "^2.0.2",
		"html-webpack-plugin": "^5.5.3",
		"mini-css-extract-plugin": "^2.7.6",
		"npm-run-all": "^4.1.5",
		"postcss": "^8.4.29",
		"postcss-loader": "^7.3.3",
		"rimraf": "^5.0.1",
		"sass": "^1.66.1",
		"sass-loader": "^13.3.2",
		"style-loader": "^3.3.3",
		"tailwindcss": "^3.3.3",
		"ts-loader": "^9.4.4",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2",
		"wait-on": "^7.0.1",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4"
	}
}
